'''

if __name__ == '__main__':

    #要素の挿入 insert
    a = [1,2,3,5]
    print(a)
    a.insert(3,4)
    print(a)

    #要素の削除 remove
    a.remove(4)
    print(a)

    #要素の削除 del  (インデックスを指定)
    del a[0:3]
    print(a)

    #コピー
    #深いコピーにはcopyモジュールのインポートが必要(deepcopy)
    original = [1,2,3,4,5]
    copy = original[0:5]
    print(original)
    print(copy)

    #リストの比較
    print(original == copy)     #要素の同一性の比較
    print(original is copy)     #オブジェクトの同一性を比較
