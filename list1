'''

if __name__ == '__main__':

    a = [1,2,3,4]
    print(a)

    #rangeはiteratorを返すのでlistに変換
    b = range(1,5)
    print(list(b))

    #リストaのスライス
    a = a[0:2]
    print(a)

    #末尾に追加(文) .append()
    a.append('Hello')
    print(a);

    #インデックス指定 .index()
    c = list(range(10))
    print(c.index(8))

    #要素包含判定 in
    print(3 in c)
    print(11 in c)

    #リストの長さ len
    print(len(c))

    #最大最小 max,min
    print(max(c))
    print(min(c))
